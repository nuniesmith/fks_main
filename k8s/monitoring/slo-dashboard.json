{
  "dashboard": {
    "title": "FKS SLO Dashboard",
    "panels": [
      {
        "title": "fks_api - Availability",
        "targets": [
          {
            "expr": "fks_api:availability:ratio * 100",
            "legendFormat": "Availability %"
          }
        ],
        "type": "graph"
      },
      {
        "title": "fks_api - Error Budget Remaining",
        "targets": [
          {
            "expr": "fks_api:error_budget:remaining * 100",
            "legendFormat": "Budget %"
          }
        ],
        "type": "graph",
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [
                  20
                ],
                "type": "lt"
              }
            }
          ]
        }
